            <ProjectItem ReplaceParameters="false" TargetFileName="logo150x150.scale-100.png">logo150x150.scale-100.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="logo310x310.scale-100.png">logo310x310.scale-100.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="logo44x44.scale-100.png">logo44x44.scale-100.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="logo50x50.scale-100.png">logo50x50.scale-100.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="logo71x71.scale-100.png">logo71x71.scale-100.png</ProjectItem>
