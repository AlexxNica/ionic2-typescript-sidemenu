<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Ionic 2 (Beta 10) Sidemenu</Name>
    <Description>Template to start an Ionic 2 Beta 10 Sidemenu app</Description>
    <ProjectType>TypeScript</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>ionic2-ts-sidemenu</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.png</Icon>
    <CreateInPlace>true</CreateInPlace>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="ionic2-ts-sidemenu.jsproj" File="ionic2-ts-sidemenu.jsproj" ReplaceParameters="true">      
      <Folder Name="app" TargetFolderName="app">
        <Folder Name="pages" TargetFolderName="pages">
          <Folder Name="page1" TargetFolderName="page1">
            <ProjectItem ReplaceParameters="true" TargetFileName="page1.html">page1.html</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="page1.scss">page1.scss</ProjectItem>
            <ProjectItem ReplaceParameters="true" TargetFileName="page1.ts">page1.ts</ProjectItem>
          </Folder>
          <Folder Name="page2" TargetFolderName="page2">
            <ProjectItem ReplaceParameters="true" TargetFileName="page2.html">page2.html</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="page2.scss">page2.scss</ProjectItem>
            <ProjectItem ReplaceParameters="true" TargetFileName="page2.ts">page2.ts</ProjectItem>
          </Folder>
        </Folder>
        <Folder Name="theme" TargetFolderName="theme">
          <ProjectItem ReplaceParameters="false" TargetFileName="app.core.scss">app.core.scss</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="app.ios.scss">app.ios.scss</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="app.md.scss">app.md.scss</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="app.variables.scss">app.variables.scss</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="app.wp.scss">app.wp.scss</ProjectItem>
        </Folder>
        <ProjectItem ReplaceParameters="true" TargetFileName="app.html">app.html</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="app.ts">app.ts</ProjectItem>
      </Folder>
      <Folder Name="hooks" TargetFolderName="hooks">
        <Folder Name="after_prepare" TargetFolderName="after_prepare">
          <ProjectItem ReplaceParameters="true" TargetFileName="010_add_platform_class.js">010_add_platform_class.js</ProjectItem>
        </Folder>
        <ProjectItem ReplaceParameters="false" TargetFileName="README.md">README.md</ProjectItem>
      </Folder>
      <Folder Name="resources" TargetFolderName="resources">
        <Folder Name="android" TargetFolderName="android">
          <Folder Name="icon" TargetFolderName="icon">
            <ProjectItem ReplaceParameters="false" TargetFileName="drawable-hdpi-icon.png">drawable-hdpi-icon.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="drawable-ldpi-icon.png">drawable-ldpi-icon.png</ProjectItem>
            <ProjectItem ReplaceParameters="false" TargetFileName="drawable-mdpi-icon.png">drawable-mdpi-icon.png</ProjectItem>
